<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension id="org.jboss.tools.m2e.wro4j.internal.jshintmarker"
		name="Wro4J JSHint problem"
		point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.core.resources.problemmarker" />
		<persistent value="false" /> <!-- do not keep marker across workbench shutdown and startup-->
	</extension>
   <extension
         point="org.eclipse.m2e.core.projectConfigurators">
      <configurator
            class="org.jboss.tools.m2e.wro4j.internal.Wro4jProjectConfigurator"
            id="org.jboss.tools.m2e.wro4j.wro4jConfigurator"
            name="WRO4J Project Configurator">
      </configurator>
   </extension>
   <extension
         point="org.eclipse.m2e.core.lifecycleMappingMetadataSource">
   </extension>
</plugin>
